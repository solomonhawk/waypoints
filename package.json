    "node": "8.12.0"
  },
  "scripts": {
    "postinstall": "mkdir -p reports/junit",
    "build": "yarn run clean && webpack -p --config=configs/webpack/prod.js",
    "clean": "rm -f -r -d build",
    "lint": "tslint './src/**/*.ts*' --format stylish",
    "start": "yarn run start:dev",
    "start:dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start:prod": "yarn run build && node express.js",
    "test": "jest --coverage --config=configs/jest.json --reporters=default",
    "test:cov": "NODE_ENV=test jest --config=configs/jest.json --coverage --maxWorkers=2 --reporters=default",
    "deploy": "yarn build && now build --public",
    "deploy:now": "now build --public"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
